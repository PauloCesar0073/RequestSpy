{
  "manifest_version": 2,
  "name": "RequestSpy",
  "version": "1.0",
  "icons": {
    "128": "icone.png"
  },
  "description": "Bisbilhotar as requisições feita pelo navegador.",
  "permissions": ["webRequest", "<all_urls>", "webNavigation", "tabs", "management","webRequestBlocking"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
